NODE_ENV="development"
NEXTJS_ENV="development"

NEXT_PUBLIC_BASE_URL="http://localhost:8787"

LOG_LEVEL=DEBUG

IMAGE_MODEL="runware:100@1"

NEXT_PRIVATE_DEBUG_CACHE=1

# API Keys (only required when using specific providers)
# OpenAI API Key (required when IMAGE_PROVIDER=openai)
OPENAI_API_KEY="dummy_openai_api_key"

# Runware API Key (required when IMAGE_PROVIDER=runware)
RUNWARE_API_KEY="dummy_runware_api_key"

# Cloudflare
CLOUDFLARE_ACCOUNT_ID="dummy_cloudflare_account_id"
R2_ACCESS_KEY_ID="dummy_r2_access_key_id"
R2_SECRET_ACCESS_KEY="dummy_r2_secret_access_key"
CLOUDFLARE_D1_TOKEN="dummy_cloudflare_d1_token"
CLOUDFLARE_DATABASE_ID="dummy_cloudflare_database_id"