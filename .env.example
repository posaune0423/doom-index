NODE_ENV=development
NEXTJS_ENV=development

LOG_LEVEL=DEBUG

# Image Generation Provider
# Options: "mock" | "openai" | "replicate" | "runware"
# Default: "mock"
IMAGE_PROVIDER=mock

# Prompt Template Selection
# Options: "default" | "experimental"
# Default: "default"
PROMPT_TEMPLATE=default

# API Keys (only required when using specific providers)
# OpenAI API Key (required when IMAGE_PROVIDER=openai)
OPENAI_API_KEY=

# Replicate API Key (required when IMAGE_PROVIDER=replicate)
REPLICATE_API_KEY=

# Runware API Key (required when IMAGE_PROVIDER=runware)
RUNWARE_API_KEY=
